@model IEnumerable<ProgettoSettimanale.Models.Cliente>

<h2 class="mb-4">Clienti
</h2>

<div class="table-responsive">
    <table class="table table-striped table-hover">
        <thead class="table-dark">
            <tr>
                <th>ID</th>
                <th>Nome</th>
                <th>Cognome</th>
                <th>CF</th>
                <th>Email</th>
                <th>Telefono</th>
                <th>Cellulare</th>
                <th>Città</th>
                <th>Provincia</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var cliente in Model)
            {
                <tr>
                    <td>@cliente.Id</td>
                    <td>@cliente.Nome</td>
                    <td>@cliente.Cognome</td>
                    <td>@cliente.CodiceFiscale</td>
                    <td>@cliente.Email</td>
                    <td>@cliente.Telefono</td>
                    <td>@cliente.Cellulare</td>
                    <td>@cliente.Citta</td>
                    <td>@cliente.Provincia</td>
                </tr>
            }
        </tbody>
    </table>
</div>
